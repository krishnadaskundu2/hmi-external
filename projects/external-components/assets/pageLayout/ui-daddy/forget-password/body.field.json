{
  "forgetInitFields": [
    {
      "value": "Forget Password",
      "baseProperties": {
        "name": "forgetPass",
        "id": "forgetPass",
        "type": "staticText",
        "formName": "forgetPassForm"
      },
      "expressionProperties": {
        "visible": "",
        "visibleValue": true
      },
      "layoutProperties": {
        "css": {
          "configuration": {
            "selectorName": ".element-child",
            "xs": {
              "text-align": "center",
              "color": "white",
              "parent-font-size": "30px"
            }
          },
          "customClassName": "element-child"
        }
      }
    },
    {
      "baseProperties": {
        "name": "emailId",
        "id": "emailId",
        "type": "email",
        "formName": "forgetPassForm"
      },
      "layoutProperties": {
        "selectorName": ".email__field",
        "css": {
          "md": {
            "border": "1px solid red"
          },
          "customClassName": "email__field"
        }
      },
      "placeholder": "Enter Your Email",
      "accessor": "obj.email",
      "expressionProperties": {
        "requiredValue": false,
        "required": "",
        "readOnly": "",
        "readOnlyValue": false,
        "disabled": "otpVerified == 'true' or otpSent == 'true'",
        "disabledValue": false,
        "visible": "",
        "visibleValue": true
      }
    },
    {
      "baseProperties": {
        "name": "otp",
        "id": "otp",
        "formName": "forgetPassForm",
        "type": "number"
      },
      "placeholder": "Enter OTP",
      "accessor": "obj.otp",
      "expressionProperties": {
        "requiredValue": false,
        "required": "",
        "readOnly": "",
        "readOnlyValue": false,
        "disabled": "",
        "disabledValue": false,
        "visible": "otpSent == 'true' and otpVerified != 'true'",
        "visibleValue": true
      },
      "minRequirementPattern": "^.{6}$",
      "hideSpinner": true
    },
    {
      "baseProperties": {
        "name": "generateOtp",
        "id": "generateOtp",
        "type": "submit",
        "formName": "forgetPassForm",
        "label": "Generate OTP"
      },
      "accessor": "obj.generateOtp",
      "expressionProperties": {
        "requiredValue": false,
        "required": "",
        "readOnly": "",
        "readOnlyValue": false,
        "disabled": "",
        "disabledValue": false,
        "visible": "otpSent != 'true' and otpVerified != 'true'",
        "visibleValue": true
      },
      "layoutProperties": {
        "css": {
          "themeClass": "btn-warning",
          "configuration": {
            "selectorName": ".signup__generate-otp",
            "xs": {
              "parent-justify-content": "center"
            }
          },
          "customClassName": "signup__generate-otp"
        }
      },
      "apiConfig": {
        "name": "generateOTPApi",
        "url": "/rest/forgotPasswordOtp",
        "method": "POST",
        "payloadParams": [
          {
            "paramName": "mailId",
            "mappedField": "emailId"
          }
        ],
        "onSuccess": {
          "showSuccessMessage": true,
          "sharedData": [
            {
              "varName": "otpSent",
              "staticData": "true"
            }
          ]
        },
        "onError": {
          "showErrorMessage": true
        }
      },
      "events": [
        {
          "event": "click",
          "actions": [
            {
              "actionType": "submit",
              "showLoading": true
            }
          ],
          "debounce": 3000
        }
      ]
    },
    {
      "baseProperties": {
        "name": "verifyOtp",
        "id": "verifyOtp",
        "type": "submit",
        "formName": "forgetPassForm",
        "label": "Verify OTP"
      },
      "accessor": "obj.verifyOtp",
      "expressionProperties": {
        "requiredValue": false,
        "required": "",
        "readOnly": "",
        "readOnlyValue": false,
        "disabled": "",
        "disabledValue": false,
        "visible": "otpSent == 'true' and otpVerified != 'true'",
        "visibleValue": true
      },
      "layoutProperties": {
        "css": {
          "themeClass": "btn-warning",
          "configuration": {
            "selectorName": ".signup__generate-otp",
            "xs": {
              "parent-justify-content": "center"
            }
          },
          "customClassName": "signup__generate-otp"
        }
      },
      "apiConfig": {
        "name": "verifyOTPApi",
        "url": "/rest/verifyOtp",
        "method": "POST",
        "payloadParams": [
          {
            "paramName": "mailId",
            "mappedField": "emailId"
          },
          {
            "paramName": "otp",
            "mappedField": "otp"
          }
        ],
        "onSuccess": {
          "showSuccessMessage": true,
          "sharedData": [
            {
              "varName": "otpVerified",
              "staticData": "true"
            }
          ]
        },
        "onError": {
          "showErrorMessage": true
        }
      },
      "events": [
        {
          "event": "click",
          "actions": [
            {
              "actionType": "submit",
              "showLoading": true
            }
          ],
          "debounce": 3000
        }
      ]
    }
  ],
  "validOtp": [
    {
      "baseProperties": {
        "name": "password",
        "id": "password",
        "formName": "forgetPassForm",
        "type": "password"
      },
      "placeholder": "Enter password",
      "accessor": "obj.pass",
      "expressionProperties": {
        "requiredValue": false,
        "required": "",
        "readOnly": "",
        "readOnlyValue": false,
        "disabled": "",
        "disabledValue": false,
        "visible": "otpVerified == 'true'",
        "visibleValue": true
      },
      "minRequirementPattern": "^.{8,20}$"
    },
    {
      "baseProperties": {
        "name": "cnfpassword",
        "id": "cnfpassword",
        "formName": "forgetPassForm",
        "type": "password"
      },
      "placeholder": "Enter confirm password",
      "accessor": "obj.cnfpass",
      "expressionProperties": {
        "requiredValue": false,
        "required": "",
        "readOnly": "",
        "readOnlyValue": false,
        "disabled": "",
        "disabledValue": false,
        "visible": "otpVerified == 'true'",
        "visibleValue": true
      },
      "minRequirementPattern": "^.{8,20}$"
    },
    {
      "baseProperties": {
        "name": "button",
        "id": "button",
        "formName": "forgetPassForm",
        "type": "submit",
        "label": "Reset"
      },
      "expressionProperties": {
        "requiredValue": false,
        "required": "",
        "readOnly": "",
        "readOnlyValue": false,
        "disabled": "",
        "disabledValue": false,
        "visible": "otpVerified == 'true'",
        "visibleValue": true
      },
      "layoutProperties": {
        "css": {
          "themeClass": "btn-warning",
          "configuration": {
            "selectorName": ".signup__btn",
            "xs": {
              "parent-justify-content": "center"
            }
          },
          "customClassName": "signup__btn"
        }
      },
      "apiConfig": {
        "name": "forgetPassApi",
        "url": "/rest/resetPassword",
        "method": "POST",
        "payloadParams": [
          {
            "paramName": "mailId",
            "mappedField": "emailId"
          },
          {
            "paramName": "password",
            "mappedField": "password"
          }
        ],
        "onSuccess": {
          "showSuccessMessage": true,
          "pageUrl": "login"
        },
        "onError": {
          "showErrorMessage": true
        }
      },
      "events": [
        {
          "event": "click",
          "actions": [
            {
              "actionType": "submit",
              "showLoading": true
            }
          ],
          "debounce": 3000
        }
      ]
    }
  ]
}
